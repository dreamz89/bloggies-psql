<h1>My posts by <%= current_user.name %></h1>

<ul>
  <% @all_posts.each do |post| %>
  <li>
    <h4><%= post.title %></h4>
    <p>
      <%= post.content %>
    </p>
  </li>
  <% end  %>
</ul>

<h2>Create new post</h2>
<%= form_for @new_post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>

  <%= f.label :content %>
  <%= f.text_field :content %>
  <br>

  <%= f.submit %>
<% end %>
